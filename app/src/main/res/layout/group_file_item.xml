<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/group_distance_5"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/m_iv_issue_file_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/group28" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/group_distance_5"
            android:layout_toLeftOf="@id/m_iv_issue_file_delete"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/group29" />

            <TextView
                android:id="@+id/m_tv_issue_result_file_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/group_distance_15"
                android:text="单元测试卷"
                android:textSize="@dimen/group_text_14" />

        </LinearLayout>


    </RelativeLayout>

</LinearLayout>