<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="tab_item">
        <attr name="src" format="reference" />
        <attr name="text" format="string" />
        <attr name="desc" format="string" />
    </declare-styleable>

    <declare-styleable name="roundView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LyricView">
        <attr name="hint" format="string" />
        <attr name="hintColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="highlightColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="maxLength" format="dimension" />
        <attr name="fadeInFadeOut" format="boolean" />
        <attr name="lineSpace" format="dimension" />
        <attr name="textAlign">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="honour_ability_view">
        <attr name="honour_ability_line_color" format="color" />
        <attr name="honour_ability_line_width" format="dimension" />
        <attr name="honour_ability_dot_color" format="color" />
        <attr name="honour_ability_dot_radius" format="dimension" />
        <attr name="honour_ability_dot_width" format="dimension" />
        <attr name="honour_ability_cover_color" format="color" />
        <attr name="honour_ability_cover_width" format="dimension" />
        <attr name="honour_ability_text_color" format="color" />
        <attr name="honour_ability_text_size" format="dimension" />
        <attr name="honour_ability_center_text_size" format="dimension" />
        <attr name="honour_ability_center_text_color" format="dimension" />
        <attr name="honour_ability_text_offset" format="dimension" />
    </declare-styleable>
    <declare-styleable name="vip_item">
        <attr name="vip_icon" format="reference" />
        <attr name="vip_content" format="string" />
        <attr name="vip_content_size" format="dimension" />
        <attr name="vip_content_color" format="color" />
    </declare-styleable>

    <declare-styleable name="tutorship_detail_view">
        <attr name="title" format="string" />
        <attr name="title_bg_color" format="color" />
        <attr name="bad_content" format="string" />
        <attr name="good_content" format="string" />
    </declare-styleable>

</resources>