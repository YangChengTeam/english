<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yc.english">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".EnglishApp"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name=".main.view.activitys.MainActivity"
            android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".read.view.activitys.BookActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".read.view.activitys.CoursePlayActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".read.view.activitys.AddBookActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".read.view.activitys.BookUnitActivity" android:theme="@style/AppTheme"/>

        <activity android:name=".group.view.activitys.GroupCreateActivity"/>
        <activity android:name=".group.view.activitys.GroupJoinActivity"/>

    </application>

</manifest>