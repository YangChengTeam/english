<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.yc.english"

          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name=".EnglishApp"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".main.view.activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".read.view.activitys.CoursePlayActivity">

        </activity>
        <activity android:name=".group.view.activitys.GroupCreateActivity"/>
        <activity android:name=".group.view.activitys.GroupJoinActivity"/>

    </application>

</manifest>